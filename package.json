{
  "name": "@molfar/amqp-client",
  "version": "1.0.0",
  "description": "Molfar AMQP Client based on AMQP 0-9-1 library",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest --no-cache --runInBand --forceExit --passWithNoTests",
    "example":"node example",
    "docs:api": "jsdoc2md  --template docs/.config/README.hbs --files lib/**/*.js  > docs/api/README.md",
    "docs:schemas": "node ./build/docs/prepare && jsonschema2md -d ./docs/schemas/json -o ./docs/schemas -p version,test -x ./docs/schemas/json",
    "docs:build": "npm run test && npm run docs:api && npm run docs:schemas",
    "docs:dev": " npm run docs:build && npx vuepress dev docs",
    "docs:prepublish": "npm run docs:build && npx vuepress build docs",
    "docs:gh-pages": "gh-pages.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wdc-molfar/amqp-client.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wdc-molfar/amqp-client/issues"
  },
  "homepage": "https://github.com/wdc-molfar/amqp-client#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^27.2.5",
    "jest-html-reporters": "^2.1.6",
    "jest-openapi": "^0.14.0",
    "jest-stare": "^2.3.0",
    "jsdoc-to-markdown": "^7.0.1",
    "uuid": "^8.3.2",
    "vuepress": "^1.8.2",
    "vuepress-theme-cool": "^1.3.1",
    "widdershins": "^4.0.1"
  },
  "jest": {
    "testMatch": [
      "**/?(*.)+(test).js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov"
    ],
    "coverageDirectory": "./docs/.vuepress/public/coverage",
    "reporters": [
      "default",
      [
        "./node_modules/jest-html-reporters",
        {
          "publicPath": "./docs/.vuepress/public/testReport",
          "filename": "test-report.html",
          "pageTitle": "@molfar/ms-registry"
        }
      ]
    ]
  },
  "dependencies": {
    "@adobe/jsonschema2md": "^6.1.4",
    "@cloudamqp/amqp-client": "^1.1.6",
    "ajv": "^8.6.3",
    "ajv-errors": "^3.0.0",
    "amqplib": "^0.8.0",
    "amqplib-plus": "^1.0.9",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "require-yml": "^2.0.0"
  }
}
